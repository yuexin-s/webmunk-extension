{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.2",
    "description": "Injects a floating box on LinkedIn profiles with randomized framing.",
    "content_scripts": [
        {
            "matches": ["https://www.linkedin.com/in/*"],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ]
}
